{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "ES2015",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "LIB/*": ["src/canvas/lib/*"]
        },
        "lib": ["es2015", "es2017", "dom"]
    },
    "include": ["./src/**/*"]
}
